{
    "name": "shaQR",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "shaQR.png"
    },
    "icons":{
        "128":"shaQR.png"
    },
    "background": {
        "service_worker": "background.js"
      },
      "permissions": [
        "contextMenus",
        "notifications",
        "scripting",
        "tabs",
        
        "downloads",
        "activeTab"
      ]
}